                   (5, "Business Enterprise ATP", 2, '["ENTERPRISEPACK", "ATP_ENTERPRISE"]', 51200),
                   (6, "Enterprise E3", 1, '["ENTERPRISEPACK"]', 51200),
                   (7, "Enterprise E1", 1, '["EXCHANGESTANDARD"]', 51200),
                   (8, "Business Essentials", 1, '["O365_BUSINESS_ESSENTIALS"]', 51200),
                   (9, "Business Premium ATP", 2, '["O365_BUSINESS_PREMIUM", "ATP_ENTERPRISE"]', 51200),
                   (10, "Business Premium", 1, '["O365_BUSINESS_PREMIUM"]', 51200),
                   (11, "Power Bi", 1, '["POWER_BI_PRO"]', 51200),
                   (12, "Power Bi", 1, '["POWER_BI_STANDARD"]', 51200),
