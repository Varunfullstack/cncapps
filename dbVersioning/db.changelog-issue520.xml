            </not>
        </preConditions>
        <addColumn tableName="headert">
            <column type="int(3)" name="office365MailboxRedWarningThreshold" defaultValue="95">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>