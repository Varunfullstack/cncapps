    <changeSet author="pavwebdev" id="issue215-1528799226-1">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="contact" columnName="othersEmailFlag"/>
            </not>
        </preConditions>
        <addColumn tableName="contact">
            <column name="othersEmailFlag" type="char(1)" defaultValue="Y"/>
        </addColumn>
    </changeSet>
    <changeSet author="pavwebdev" id="issue215-1528799226-2">
        <preConditions onFail="MARK_RAN">
            <columnExists tableName="contact" columnName="othersEmailFlag"/>
        </preConditions>
        <sql>
            UPDATE contact AS c INNER JOIN customer ON c.`con_custno` = customer.`cus_custno` AND c.`con_mailflag10` =
            'Y' SET c.othersEmailFlag = customer.`cus_others_email_main_flag`
        </sql>
        <rollback/>
    </changeSet>
    <changeSet author="pavwebdev" id="issue215-1528799226-3">
        <preConditions>
            <columnExists tableName="customer" columnName="cus_others_email_main_flag"/>
        </preConditions>
        <dropColumn tableName="customer" columnName="cus_others_email_main_flag"/>
        <rollback>
            <addColumn tableName="customer">
                <column name="cus_others_email_main_flag" type="char(1)" defaultValue="Y"/>
            </addColumn>
            <sql>
                update customer set customer.cus_others_email_main_flag = (select c.othersEmailFlag from contact as c
                where c.con_custno = customer.cus_custno and c.con_mailflag10 = 'Y' limit 1)
            </sql>
        </rollback>
    </changeSet>
