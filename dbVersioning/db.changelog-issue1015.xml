    <changeSet id="issue1015-1" author="pavwebdevmt">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="consultant" columnName="execludeFromSDManagerDashboard"/>
            </not>
        </preConditions>
